{
  "name": "url-shortener",
  "author": "Matheus Figueredo <matheus.figueredo@gmail.com>",
  "version": "1.0.0",
  "description": "An URL Shortener with Express + ES6",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js --exec \"node -r dotenv/config -r babel-register\"",
    "test": "jest --setupFiles dotenv/config"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "mongoose": "^5.4.13",
    "morgan": "^1.9.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^24.1.0",
    "mockingoose": "^2.12.0",
    "nodemon": "^1.18.10",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
